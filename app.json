{
  "expo": {
    "name": "cricket-umpire-ball-counter",
    "slug": "cricket-umpire-ball-counter",
    "version": "5.1.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "cricketumpireballcounter",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "expo-router": {
      "source": "src/app"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.4dot6.cricketballandovercounter",
      "buildNumber": "5.1.3",
      "icon": "./assets/ios-icon.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#12c2e9",
        "foregroundImage": "./assets/ios-icon.png",
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cricketovercounterapp",
      "versionCode": 47
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon-4dot6.png",
          "resizeMode": "contain",
          "backgroundColor": "#12c2e9",
          "dark": {
            "backgroundColor": "#12c2e9"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01f25192-0a2a-4868-a640-b12a9dd6b98d"
      }
    }
  }
}
